        <div class="section section-contact-us text-center">
            <div class="container">
                <h2 class="title">Want to Discuss something?</h2>
                {{#if hasSuccess}}
                 <div class="alert alert-success" role="alert">
                {{# each messages}}
                    <div class="container">
                            <div class="alert-icon">âœ”</div>
                            {{this}}
                    </div>
                    <button type="button" class="close" data-dismiss="alert"
                                    aria-label="Close">
                                        <span aria-hidden="true">
                                            &times;
                                        </span>
                    </button>
                    <script>
                        swal("Enquiry Sent!", " {{this}}!", "success");
                    </script>
                {{/each}}
                </div>
                {{/if}}
                <form class="form" action="/" method="POST">
                <div class="row">
                    <div class="col-lg-6 text-center col-md-8 ml-auto mr-auto">
                        <div class="input-group input-lg">
                            <div class="input-group-prepend">
                        <span class="input-group-text">
                          <i class="fa fa-user" aria-hidden="true"></i>
                        </span>
                            </div>
                            <input type="text" class="form-control" id="firstname" name="firstname" placeholder="First Name..." required>
                        </div>
                        <div class="input-group input-lg">
                            <div class="input-group-prepend">
                        <span class="input-group-text">
                            <i class="fa fa-envelope" aria-hidden="true"></i>
                        </span>
                            </div>
                            <input type="text" class="form-control" id="email" name="email"  placeholder="Email..." required>
                        </div>
                        <div class="textarea-container">
                            <textarea class="form-control" id="message" name="message"  rows="4" cols="80"
                                      placeholder="Type a message..." required></textarea>
                        </div>
                        <div class="send-button">
                            <button type="submit" class="btn btn-primary btn-round btn-block btn-lg">Send Message <i class="fa fa-paper-plane" aria-hidden="true"></i>
                            </button>
                        </div>
                    </div>
                </div>
                </form>
            </div>
        </div>
        <div class="section section-contact-us text-center">
            <div class="container">
                <h2 class="title">Visit us</h2>
                <iframe src="https://www.google.com/maps/embed?pb=!1m14!1m8!1m3!1d14906.105971713616!2d72.9437967!3d20.9313617!3m2!1i1024!2i768!4f13.1!3m3!1m2!1s0x0%3A0x4b358d597b685106!2sCM%20Classes!5e0!3m2!1sen!2sin!4v1592847490822!5m2!1sen!2sin" width="1080" height="450" frameborder="0" style="border:0;" allowfullscreen="" aria-hidden="false" tabindex="0"></iframe>
