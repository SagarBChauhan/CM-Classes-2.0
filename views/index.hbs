                {{# each messages}}
                    <div class="container">
                            <div class="alert-icon">âœ”</div>
                            {{this}}
                    </div>
                    <button type="button" class="close" data-dismiss="alert"
                                    aria-label="Close">
                                        <span aria-hidden="true">
                                            &times;
                                        </span>
                    </button>
                    <script>
                        swal("Enquiry Sent!", " {{this}}!", "success");
                    </script>
                {{/each}}
                </div>
                {{/if}}
                <form class="form" action="/" method="POST">
